{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "<o>{{ .Segments.Time.CurrentDate | date .Segments.Time.Format }} <#ecf028>{{.UserName}}@{{.HostName}}{{if .Root}}<,#ff0f0f><b>{{end}}{{if .Segments.Session.SSHSession}}<u>À¢À¢ ∞{{end}}{{if .Root }} ‚ö°{{end}}</u></b><,transparent> {{if .Segments.Git }}<#74c93d>{{.Segments.Git.HEAD}} {{end}}<#56d2d3>{{.Segments.Path.Path}}{{.Env.SLASH}}",
    "foreground": "#7298e2",
    "foreground_templates": [
      ""
    ],
    "background_templates": [
      ""
    ]
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "<#7c7c7c> | </>",
          "template": "{{if .Root}}<b>{{end}}{{if .SSHSession}}<u>À¢À¢ ∞{{end}}{{ .UserName }}@{{ .HostName }}{{if .Root }} ‚ö°{{end}}</u></b>",
          "foreground": "#ecf028",
          "type": "session",
          "style": "diamond",
          "foreground_templates": [
            "{{if .SSHSession }}#e275ab{{end}}",
            "{{if .Root }}#ffffff{{end}}"
          ],
          "background_templates": [
            "{{if .Root }}#ff0f0f{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_distro_name": true,
            "windows": "\ue62a Windows"
          },
          "template": "{{.Icon}}{{ if .WSL }}/\ue62a{{ end }} {{ lower .Env.ARCH }}",
          "foreground": "#f58d31",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "pwsh": "\ue683pwsh"
            }
          },
          "trailing_diamond": "<#7c7c7c>|</>",
          "foreground": "#ecf028",
          "type": "shell",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "<#7c7c7c> | </>",
          "template": "{{ if ne \"1\" .Env.SHLVL }}(üêö {{.Env.SHLVL}}){{ end }}",
          "foreground": "#ecf028",
          "type": "text",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "filler": "<#4a4a6c>\u2594</>",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "threshold": 2500
          },
          "trailing_diamond": "<#7c7c7c> | </>",
          "template": "took \uf252 {{ .FormattedMs }}",
          "foreground": "#b62424",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "<#7c7c7c>|</>",
          "template": "\ue718{{ .Full }}",
          "foreground": "#ffffff",
          "background": "#6CA35E",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "<#7c7c7c>|</>",
          "template": "\ue738{{ .Full }}",
          "foreground": "#ffffff",
          "background": "#4c1f5e",
          "type": "java",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "<#7c7c7c>|</>",
          "template": "\ue738{{ .Full }}",
          "foreground": "#56d2d3",
          "type": "go",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "<#7c7c7c>|</>",
          "template": "<i>‚òÅ{{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "foreground": "#FFA400",
          "type": "aws",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "charged_icon": "\uf58e",
            "charging_icon": "\uf58f",
            "discharging_icon": "\uf58c",
            "not_charging_icon": "\uf590"
          },
          "leading_diamond": "<#7c7c7c> | </>",
          "template": "{{ if not .Error }}{{ .Percentage }}\uf295 {{ .Icon }}{{ end }}",
          "foreground": "#ffeb3b",
          "type": "battery",
          "style": "plain",
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff7944{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "branch_icon": "\uf126 ",
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "github_icon": "\uf113 "
          },
          "trailing_diamond": "<#7c7c7c> | </>",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}",
          "foreground": "#74c93d",
          "type": "git",
          "style": "diamond",
          "foreground_templates": [
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#16c60c{{ end }}",
            "{{ if gt .Behind 0 }}#f450de{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "{{$Blah := \"foobar\"}}\uf115 {{ .Path }}{{.Env.SLASH}}",
          "foreground": "#77E4F7",
          "type": "path",
          "style": "plain",
          "foreground_templates": [
            "{{if .Root }}#ff5f5f{{end}}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " {{ if gt .Code 0 }}<#ff0000>{{.Code}} \uf00d</>{{ else }}<#23d18b>\uf42e</>{{ end }}",
          "type": "status",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "Jan 02 03:04:05 PM"
          },
          "leading_diamond": "<#7c7c7c> | </>",
          "template": "{{ .CurrentDate | date .Format }} \uf017",
          "foreground": "#6288d2",
          "type": "time",
          "style": "diamond"
        }
      ]
    }
  ],
  "version": 3,
  "shell_integration": true
}
